{
  "name": "terra-dominus-frontend",
  "version": "1.0.0",
  "description": "Frontend for Terra Dominus",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "CI=true react-scripts test --watch=false",
    "test:e2e": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint:a11y": "eslint \"src/**/*.{js,jsx}\"",
    "serve:prod": "serve -s build -l 3000",
    "start:prod": "npm run serve:prod",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.11.5",
    "@emotion/styled": "^11.11.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^1.7.2",
    "lucide-react": "^0.555.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "serve": "^14.2.3"
  },
  "proxy": "http://localhost:5000",
